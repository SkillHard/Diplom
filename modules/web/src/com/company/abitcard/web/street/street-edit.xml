<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.abitcard.web.street.StreetEdit"
        datasource="streetDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.abitcard.web.street">
    <dsContext>
        <datasource id="streetDs"
                    class="com.company.abitcard.entity.Street"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions" spacing="true">
        <fieldGroup id="fieldGroup" datasource="streetDs">
            <column width="250px">
                <field property="street"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
